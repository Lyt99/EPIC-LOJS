<Window x:Class="EPIC_Core.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EPIC_Core"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="585">
    <Grid Margin="0,0,2,0">
        <Label x:Name="label" Content="Path of exe:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="87,14,0,0" TextWrapping="Wrap" Text="C:\Users\Family\Desktop\1.exe" VerticalAlignment="Top" Width="478" TextChanged="textBox_TextChanged"/>
        <Button x:Name="button" Content="Run" HorizontalAlignment="Left" Margin="490,42,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
